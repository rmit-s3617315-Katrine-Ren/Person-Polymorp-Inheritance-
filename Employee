public abstract class Employee extends Person {


	private String taxFileNumber;
	
	public Employee(String name, String address, String tfn) {
		super(name, address);
		taxFileNumber = tfn;
		System.out.printf("Initilize the Employee object with tfn: "
				+ "%s\n", tfn);
	}
	
	public String getTaxFileNumber() {
		return taxFileNumber;
	}


}

public class ContractEmployee extends Employee {


	private double hourlyRate;
	
	public ContractEmployee(String name, String address, 
			String tfn, double hRate) {
		super(name, address, tfn);
		hourlyRate = hRate;
		System.out.printf("Initilize the Contract Employee object "
				+ "with hourly rate: %f\n", hRate);
	}


}

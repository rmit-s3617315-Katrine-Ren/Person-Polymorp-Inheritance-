public class SalaryEmployee extends Employee {


	private double yearlySalary;
	
	public SalaryEmployee(String name, String address, 
			String tfn, double ySalary) {
		super(name, address, tfn);
		yearlySalary = ySalary;
		System.out.printf("Initilize the Salary Employee object "
				+ "with yearly salary: %f\n", ySalary);
	}


}


